<!-- inside app/views/index.html.erb -->  

<main>
  <div class="container">
    <div class="container py-4">

      <% if current_user %>
        <%= render "partials/header_logged" %>
      

      <% else %>
        <%= render "partials/header_unlogged" %>
        <%= render "partials/wellcome" %>
      <% end %>

      <%= render "partials/footer"%>

    </div>
  </div>
</main>